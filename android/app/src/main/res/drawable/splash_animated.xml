<?xml version="1.0" encoding="utf-8"?>
<animated-vector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt"
    xmlns:tools="http://schemas.android.com/tools"
    tools:targetApi="lollipop">
    <aapt:attr name="android:drawable">
        <vector
            android:width="108dp"
            android:height="108dp"
            android:viewportWidth="880"
            android:viewportHeight="960">

            <path
                android:fillColor="#F04C4E"
                android:pathData="M0,0h1000v1000h-1000z" />

            <path
                android:pathData="M492 640c4-3 9-4 12-3l3 1 2 2A235 235 0 1 1 192 515l7 25a210 210 0 1 0 291 120c-2-6-2-11-1-15 0-3 2-5 3-5Z"
                android:strokeWidth="79"
                android:strokeColor="#ffffff" />

            <group
                android:name="spiral_group"
                android:pivotX="600"
                android:pivotY="480">
                <path
                    android:fillColor="#ffffff"
                    android:pathData="M416 371c-82-22-123-33-145-10-23 22-12 63 10 145l5 17c22 82 33 123 63 131 31 8 61-21 121-81l13-13c60-60 90-90 81-121-8-30-49-41-131-63l-17-5Z" />
            </group>
        </vector>
    </aapt:attr>

    <target android:name="spiral_group">
        <aapt:attr
            name="android:animation"
            tools:ignore="UnusedAttribute">
            <set>
                <objectAnimator
                    android:duration="1000"
                    android:interpolator="@android:interpolator/overshoot"
                    android:propertyName="scaleX"
                    android:valueFrom="0"
                    android:valueTo="0.95"
                    android:valueType="floatType" />
                <objectAnimator
                    android:duration="1000"
                    android:interpolator="@android:interpolator/overshoot"
                    android:propertyName="scaleY"
                    android:valueFrom="0"
                    android:valueTo="0.95"
                    android:valueType="floatType" />
            </set>
        </aapt:attr>
    </target>

</animated-vector>
